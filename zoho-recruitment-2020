class Programm
{
public static void main(String [] args)
{
public static word(String str)
{
char ch[]=str.toCharArray();
char ch2[]=new char[ch.length];
int j=0;
int k=(ch.length/2);
for(int i=k;i<=ch.length-1;i++)
{
ch2[j]=ch[i];
j++;
}
for(int i=0;i<=k-1;i++)
{
Ch2[j]=ch[i];
j++;
}

int first=ch2[0];

For(int i=0;i<=ch2.length-1;i++)
{
first=0;
for(int j=0;j<=i;j++)
{
System.out.print(ch2[first]);
first++;
}
System.out.println();
}
}
}
